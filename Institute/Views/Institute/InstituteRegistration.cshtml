@{
    ViewBag.Title = "InstituteRegistration";
}


<section id="main-content">
    <section class="wrapper">
        <h3><i @*class="fa fa-angle-right"*@></i> </h3>

        <!-- INLINE FORM ELELEMNTS -->
        <div class="row mt">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="form-panel">
                    <div class="form-panel">
                        <h4 class="mb"><i class=""></i> Institute Registration</h4>
                    </div>
                    <form class="form-inline" role="form" style="margin-left:25px;">
                        <div class="row mt">
                            <div>
                                <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                    <label class="control-label" style="">Institue Name</label>
                                </div>
                                <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                    <input type="text" class="form-control " id="exampleInputEmail2" autocomplete="off">
                                </div>
                            </div>

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label ">Location</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                        </div>
                        <div class="row mt">

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Address 1</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="email" class="form-control " id="exampleInputEmail2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label " style="">Address 2</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                        </div>
                        <div class="row mt">

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Address 3</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="email" class="form-control " id="exampleInputEmail2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label  " style="">Area</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                        </div>
                        <div class="row mt">

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Building</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="email" class="form-control " id="exampleInputEmail2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label  " style="">Room No.</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                        </div>
                        <div class="row mt">

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Mobile</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="email" class="form-control " id="exampleInputEmail2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label  " style="">Email</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                        </div>
                        <div class="row mt">

                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Website</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="email" class="form-control " id="exampleInputEmail2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label  " style="">Short Name</label>
                            </div>
                            <div class="form-group col-lg-4 col-md-4 col-sm-4 mb">
                                <input type="text" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>

                        </div>
                        <div class="row mt">


                            <div class="form-group col-lg-2 col-md-2 col-sm-2 mb">
                                <label class="control-label" style="">Upload Logo</label>
                            </div>
                            <div class="form-group col-lg-3 col-md-3 col-sm-3 mb">
                                <input type="file" class="form-control" id="exampleInputPassword2" autocomplete="off">
                            </div>
                            <div class="form-group col-lg-12 col-md-12 col-sm-12 mb">
                                <button type="submit" runat="server" style="float:right;background-color:#f76f0c" class="btn btn-primary notika-btn-primary waves-effect a-prevent" name="InstituteRegistration" formmethod="post" formaction="InstituteRegistration" id="btnUpdate"><i class="fa fa-upload"></i> Update</button>
                            </div>
                        </div>


                    </form>
                </div>
                <!-- /form-panel -->
            </div>
            <!-- /col-lg-12 -->
        </div>
        <!-- /row -->
    </section>
    <!-- /wrapper -->
</section>

